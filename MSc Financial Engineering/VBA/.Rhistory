#install.packages('BatchGetSymbols')
#install.packages("writexl")
library(BatchGetSymbols)
library(writexl)
library(ggplot2)
library(quantmod)
library(xts)
mydirectory <- "C:/Users/apala/OneDrive/Documents/Python/VBA"
setwd(mydirectory)
Stocks<-c("SPY","AMZN","AAPL","WFC","MSFT","NVDA","AMD","BAC","GE","XOM","JPM","GS","C")
Prices=BatchGetSymbols(Stocks,first.date="2020-01-02", last.date ="2021-08-30", freq.data='daily')
p <- ggplot(Prices$df.tickers, aes(x = ref.date, y = price.close))
p <- p + geom_line()
p <- p + facet_wrap(~ticker, scales = 'free_y')
print(p)
Prices2<-reshape.wide(Prices$df.tickers)
write_xlsx(Prices2,"C:/Users/apala/OneDrive/Documents/Python/VBA.xlsx")
